import java.util.Scanner;

public class Main 
{
    public static void main(String[] args)
    {
        int N=0;
        Scanner sc = new Scanner(System.in);
        N = sc.nextInt();
        int revnum = 0 ;
        while (N>0)
        {
            int lastdigit = N % 10;
            N = N/10 ;
            revnum = (revnum*10) + lastdigit;
            
        }
        
        System.out.println(revnum);
        
    }
}
